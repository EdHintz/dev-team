{
  "name": "dev-team-web",
  "version": "2.0.0",
  "description": "Dev Team orchestrator web UI with real-time sprint management",
  "type": "module",
  "scripts": {
    "dev": "tsx watch server/index.ts",
    "dev:client": "vite",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:client\"",
    "build": "tsc -p tsconfig.server.json && vite build",
    "start": "node dist/server/index.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "bullmq": "^5.34.8",
    "cors": "^2.8.5",
    "dotenv": "^17.2.4",
    "express": "^4.21.2",
    "ioredis": "^5.4.2",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^2.1.8",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  }
}
